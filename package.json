{
  "name": "simple-usable-accordion",
  "version": "1.0.0",
  "description": "A Simple Usable Gutenberg Block where you can implement a very smart accordion.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src",
    "start": "webpack --mode=development --watch",
    "sync": "browser-sync start -p 'http://localhost/' --files '**/*.php' 'dist/*.js' 'dist/*.css'",
    "preview": "npm-run-all --parallel sync start",
    "i18n-pot": "wp i18n make-pot . languages/test-project.pot --exclude=\"src\"",
    "cp": "./languages/test-project.pot ./languages/test-project-en_US.po",
    "i18n-json": "wp i18n make-json languages/ --no-purge",
    "i18n-mo": "wp i18n make-mo languages/",
    "i18n": "npm run i18n-pot && npm run i18n-json && npm run i18n-mo",
    "build": "gulp clean && npm run lint && webpack --mode=production && npm run i18n && gulp bundle"
  },
  "author": "Wasek Billah",
  "license": "ISC",
  "dependencies": {
    "immer": "^10.0.4",
    "react-icons": "^4.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.7",
    "@babel/preset-react": "^7.23.3",
    "@wordpress/blob": "^3.48.0",
    "@wordpress/block-editor": "^12.16.0",
    "@wordpress/blocks": "^12.25.0",
    "@wordpress/components": "^25.14.0",
    "@wordpress/compose": "^6.25.0",
    "@wordpress/data": "^9.18.0",
    "@wordpress/element": "^5.25.0",
    "@wordpress/html-entities": "^3.48.0",
    "@wordpress/i18n": "^4.48.0",
    "@wordpress/rich-text": "^6.25.0",
    "@wordpress/server-side-render": "^4.25.0",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "browser-sync": "^3.0.2",
    "browserslist": "^4.22.2",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "del": "^7.1.0",
    "eslint": "^8.56.0",
    "eslint-config-node": "^4.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^4.0.1",
    "file-loader": "^6.2.0",
    "gulp": "^4.0.2",
    "gulp-zip": "^6.0.0",
    "mini-css-extract-plugin": "^2.7.6",
    "node-sass": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^7.3.4",
    "sass-loader": "^13.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
